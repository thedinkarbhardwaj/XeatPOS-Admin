<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

  <!--  <data>

    </data>
-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:background="@color/white"
        android:orientation="vertical">

        <ImageView android:id="@+id/imageBitmap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"/>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        <LinearLayout android:id="@+id/ll"
            android:layout_width="190dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:layout_gravity="center_horizontal"
            android:orientation="vertical"
            android:background="@color/white"
            tools:context=".activities.POblackrintActivity">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="180sp"
                    android:layout_height="70sp"
                    android:layout_marginTop="10sp"
                    android:background="@drawable/xeat_logo_new"
                    android:backgroundTint="@color/black"/>

                <TextView
                    android:id="@+id/txt_resturant_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/test"
                    android:text=""
                    android:textStyle="normal"
                    android:textColor="@color/black"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/txt_order_by"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_marginRight="5sp"
                    android:fontFamily="@font/test"
                    android:textStyle="normal"
                    android:text="Order by : "
                    android:textColor="@color/black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/txt_order_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginRight="5sp"
                    android:fontFamily="@font/test"
                    android:textStyle="normal"
                    android:text="Order ID : "
                    android:textColor="@color/black"
                    android:textSize="11sp"/>





                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5sp"
                    android:fontFamily="@font/test"
                    android:maxLines="1"
                    android:text="---------------------------------------------------------------------------------------------------"
                    android:textColor="@color/black"
                    android:textSize="7sp" />

                <TextView
                    android:id="@+id/txt_OrderType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:text=""
                    android:layout_marginTop="5sp"
                    android:layout_gravity="center_horizontal"
                    android:textStyle="normal"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:fontFamily="@font/test" />

                <TextView
                    android:id="@+id/txt_user_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginRight="5sp"
                    android:fontFamily="@font/test"
                    android:layout_marginTop="4sp"
                    android:text="Address : "
                    android:textColor="@color/black"
                    android:textSize="10sp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_user_phone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginRight="5sp"
                    android:fontFamily="@font/test"
                    android:layout_marginTop="4sp"
                    android:text="Phone : "
                    android:textColor="@color/black"
                    android:textSize="10sp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_order_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginRight="5sp"
                    android:fontFamily="@font/test"
                    android:textStyle="normal"
                    android:layout_marginTop="5sp"
                    android:layout_gravity="center_horizontal"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="11sp"/>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5sp"
                    android:maxLines="1"
                    android:fontFamily="@font/test"
                    android:text="--------------------------------------------------------------------------88-----------------------------------------"
                    android:textColor="@color/black"
                    android:textSize="7sp" />

                <LinearLayout
                    android:id="@+id/ll_container"
                    android:layout_width="180sp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:orientation="vertical">


                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Instructions:"
                    android:textStyle="normal"
                    android:layout_gravity="left"
                    android:layout_marginTop="5dp"
                    android:fontFamily="@font/test"
                    android:textColor="@color/black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/txt_InstructionSlip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="normal"
                    android:text=""
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="left"
                    android:fontFamily="@font/test"
                    android:textColor="@color/black"
                    android:textSize="10sp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5sp"
                    android:maxLines="1"
                    android:fontFamily="@font/test"
                    android:text="----------------------------------------------------------------------------------"
                    android:textColor="@color/black"
                    android:textSize="7sp" />





                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5sp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Total"
                        android:textStyle="normal"
                        android:fontFamily="@font/test"
                        android:textColor="@color/black"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/txt_total_amount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="end"
                        android:textStyle="normal"
                        android:text=""
                        android:fontFamily="@font/test"
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="13sp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Discount"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:textStyle="normal"
                        android:fontFamily="@font/test"/>


                    <TextView
                        android:id="@+id/txt_discount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5sp"
                        android:layout_marginEnd="13sp"
                        android:layout_marginRight="5sp"
                        android:gravity="end"
                        android:text=""
                        android:textStyle="normal"
                        android:fontFamily="@font/test"
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="11sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Delivery Charges"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:textStyle="normal"
                        android:fontFamily="@font/test"/>


                    <TextView
                        android:id="@+id/txt_DeliveryCharges"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5sp"
                        android:layout_marginEnd="13sp"
                        android:layout_marginRight="5sp"
                        android:gravity="end"
                        android:text=""
                        android:textStyle="normal"
                        android:fontFamily="@font/test"
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="11sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="VAT"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:textStyle="normal"
                        android:fontFamily="@font/test"/>


                    <TextView
                        android:id="@+id/txt_VAT"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5sp"
                        android:layout_marginEnd="13sp"
                        android:layout_marginRight="5sp"
                        android:gravity="end"
                        android:text=""
                        android:textStyle="normal"
                        android:fontFamily="@font/test"
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="11sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Service Charges"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:textStyle="normal"
                        android:fontFamily="@font/test"/>


                    <TextView
                        android:id="@+id/txt_ServiceCharges"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5sp"
                        android:layout_marginEnd="13sp"
                        android:layout_marginRight="5sp"
                        android:gravity="end"
                        android:text=""
                        android:textStyle="normal"
                        android:fontFamily="@font/test"
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="11sp" />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Payment mode"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:textStyle="normal"
                        android:fontFamily="@font/test" />


                    <TextView
                        android:id="@+id/txt_PaymentMode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5sp"
                        android:layout_marginEnd="13sp"
                        android:layout_marginRight="5sp"
                        android:gravity="end"
                        android:text=""
                        android:textStyle="normal"
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:fontFamily="@font/test" />

                </LinearLayout>

               <!-- <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Order Type"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:textStyle="normal"
                        android:fontFamily="@font/test"/>


                    <TextView
                        android:id="@+id/txt_OrderType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5sp"
                        android:layout_marginEnd="13sp"
                        android:layout_marginRight="5sp"
                        android:gravity="end"
                        android:text=""
                        android:textStyle="normal"
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="11sp"
                        android:fontFamily="@font/test" />

                </LinearLayout>-->




                <!--      <TextView
                          android:id="@+id/txt_thank_message"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="13sp"
                          android:fontFamily="@font/alex_brush"
                          android:text="Thank You!"
                          android:textStyle="normal"
                          android:textColor="@color/black"
                          android:textSize="14sp" />
-->

                <ImageView
                    android:id="@+id/imgQR"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_marginTop="10dp"
                    android:src="@drawable/qr_image_new"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="http://xeat.co.uk"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:layout_marginTop="10dp"
                    android:textStyle="normal"
                    android:fontFamily="@font/test"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Thank You"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="80dp"
                    android:textStyle="italic"
                    android:fontFamily="@font/test"/>




            </LinearLayout>


        </LinearLayout>

        </ScrollView>

    </LinearLayout>
</layout>