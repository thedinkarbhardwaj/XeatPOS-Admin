<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center_horizontal"
    android:paddingLeft="32sp"
    android:paddingRight="32sp"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/layoutPrint"
        android:layout_width="200sp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8sp"
        android:background="@color/white"
        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">



            <ImageView
                android:id="@+id/imageCouponImagePrint"
                android:layout_width="80sp"
                android:layout_height="80sp"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:src="@drawable/user" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10sp"
            android:gravity="center_horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="serif"
                android:text="@string/ready"
                android:textColor="@color/black"
                android:visibility="gone"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/textPlanNamePrint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="serif"
                android:text=""
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="12sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5sp"
            android:visibility="gone"
            android:gravity="center_horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="serif"
                android:text="1000$"
                android:textColor="@color/black"
                android:visibility="gone"
                android:textSize="17sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textAmountPrint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20sp"
                android:fontFamily="serif"
                android:text=""
                android:textColor="@color/black"
                android:textSize="17sp"
                android:textStyle="bold" />


        </LinearLayout>

        <LinearLayout android:id="@+id/layoutUsernamePasswodPrint"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4sp"
                    android:fontFamily="serif"
                    android:text="Username: "
                    android:padding="5sp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="11sp"/>

                <TextView
                    android:id="@+id/textUsernamePrint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4sp"
                    android:fontFamily="serif"
                    android:text=""
                    android:padding="5sp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="11sp"/>

            </LinearLayout>

       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4sp"
            android:padding="5sp"
            android:fontFamily="serif"
            android:text="Password: "
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="11sp"/>

           <TextView
               android:id="@+id/textPasswordPrint"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginTop="4sp"
               android:padding="5sp"
               android:layout_marginLeft="7dp"
               android:fontFamily="serif"
               android:text=""
               android:textColor="@color/black"
               android:textStyle="bold"
               android:textSize="11sp"/>

       </LinearLayout>

        </LinearLayout>


        <LinearLayout android:id="@+id/layoutPinPrint"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            android:fontFamily="serif"
            android:text=""
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/textPINPrint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="3sp"
            android:padding="5sp"
            android:text=""
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="18sp"/>


        </LinearLayout>

        <TextView
            android:id="@+id/textDescPrint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40sp"
            android:layout_marginRight="40sp"
            android:layout_marginTop="6sp"
            android:fontFamily="serif"
            android:text=""
            android:textStyle="bold"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="9sp"/>

        <TextView
            android:id="@+id/textSerialPrint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10sp"
            android:text="Serial "
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="13sp"/>

        <TextView
            android:id="@+id/textExpiryPrint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4sp"
            android:fontFamily="serif"
            android:text="Expiry "
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="12sp"/>



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="9sp"
            android:fontFamily="serif"
            android:text=""
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/textDateTimePrint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:padding="3sp"
            android:text=""
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageBarCodePrint"
            android:layout_width="100sp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5sp"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5sp">

            <ImageView
                android:id="@+id/imagePrintLogo"
                android:layout_width="60sp"
                android:layout_height="60sp"
                android:layout_gravity="left"
                android:layout_marginLeft="20sp"
                android:layout_marginTop="5sp"
                android:scaleType="fitXY"
                android:src="@drawable/xeat_logo" />

            <TextView android:id="@+id/textSellerName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/black"
                android:layout_centerVertical="true"
                android:textSize="11sp"
                android:textStyle="bold"
                android:layout_marginRight="20dp"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>

        <TextView android:id="@+id/textPrintCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:layout_below="@+id/textSellerName"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_marginTop="3sp"
            android:textSize="9sp"
            android:layout_gravity="center_horizontal"/>





    </LinearLayout>

</LinearLayout>